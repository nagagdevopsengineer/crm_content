{
  "kind": "collectionType",
  "collectionName": "trips",
  "info": {
    "singularName": "trip",
    "pluralName": "trips",
    "displayName": "trips",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tripdate": {
      "type": "date"
    },
    "uuid": {
      "type": "uid",
      "private": true
    },
    "starttime": {
      "type": "datetime"
    },
    "endtime": {
      "type": "datetime"
    },
    "route_bus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::route-bus.route-bus"
    },
    "isstarted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isended": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "bus_driver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bus-driver.bus-driver"
    },
    "scheduledtime": {
      "type": "datetime",
      "required": true
    },
    "Km": {
      "type": "biginteger"
    },
    "repeat": {
      "type": "integer",
      "default": 1
    }
  }
}
