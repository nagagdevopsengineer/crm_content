{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "employee",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "contact": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "is_activated": {
      "type": "boolean"
    },
    "profile_url": {
      "type": "string"
    },
    "vaccination_certificate_url": {
      "type": "string"
    },
    "route": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::route.route"
    },
    "stop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::stop.stop"
    },
    "uuid": {
      "type": "uid"
    },
    "bus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bus.bus"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "employeeotps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employeeotp.employeeotp",
      "mappedBy": "employee"
    },
    "aadhar": {
      "type": "biginteger",
      "max": "999999999999"
    },
    "aadharimage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "vaccinationcertificate": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "address": {
      "type": "text"
    },
    "dob": {
      "type": "date"
    },
    "vaccinestatus": {
      "type": "enumeration",
      "enum": [
        "No",
        "Partial",
        "Vaccinated"
      ]
    },
    "employeeid": {
      "type": "string",
      "unique": false
    },
    "client": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client"
    },
    "pushtoken": {
      "type": "string",
      "unique": false
    },
    "playerid": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "lastmile": {
      "type": "boolean",
      "default": false
    }
  }
}
