{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "employee",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "contact": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "is_activated": {
      "type": "boolean"
    },
    "profile_url": {
      "type": "string"
    },
    "vaccination_certificate_url": {
      "type": "string"
    },
    "route": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::route.route"
    },
    "stop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::stop.stop"
    },
    "uuid": {
      "type": "uid"
    },
    "bus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bus.bus"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "employeeotps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employeeotp.employeeotp",
      "mappedBy": "employee"
    },
    "aadhar": {
      "type": "biginteger",
      "max": "999999999999"
    },
    "aadharimage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
