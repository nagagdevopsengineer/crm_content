    },
    "drivinglicensefile": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "uuid": {
      "type": "uid"
    },
    "pushtoken": {
      "type": "string"
    },
    "playerid": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    }
